{
  "name": "next-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "serve": "vite --mode development.test",
    "dev": "vite --mode development.debug",
    "build": "vue-tsc --noEmit && vite build --mode production",
    "report": "vite build --mode report",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^0.2.7",
    "aqara-lib": "^2.0.0",
    "axios": "0.26.0",
    "element-plus": "^2.0.1",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "path-to-regexp": "^6.2.0",
    "uc-lib": "^2.0.0",
    "vue": "3.2.31",
    "vue-i18n": "9.1.9",
    "vue-router": "4.0.12",
    "vuex": "4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@intlify/vite-plugin-vue-i18n": "^3.3.0",
    "@ls-lint/ls-lint": "^1.11.0",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vitejs/plugin-vue-jsx": "^1.3.5",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.0",
    "less": "^4.1.2",
    "lint-staged": "^12.3.7",
    "prettier": "2.5.1",
    "rollup-plugin-visualizer": "^5.5.4",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.6.0",
    "unplugin-element-plus": "^0.3.1",
    "unplugin-vue-components": "^0.17.18",
    "vite": "^2.8.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.0.6",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^0.31.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,vue,ts,tsx}": "eslint --fix"
  },
  "eslintIgnore": [
    "**/*.md",
    "dist",
    "sensors.js",
    "index.html"
  ],
  "vite": {
    "optimizeDeps": {
      "include": [
        "aqara-lib",
        "axios",
        "element-plus",
        "element-plus/es",
        "element-plus/es/components/avatar/style/css",
        "element-plus/es/components/breadcrumb-item/style/css",
        "element-plus/es/components/breadcrumb/style/css",
        "element-plus/es/components/button/style/css",
        "element-plus/es/components/config-provider/style/css",
        "element-plus/es/components/date-picker/style/css",
        "element-plus/es/components/dropdown-item/style/css",
        "element-plus/es/components/dropdown-menu/style/css",
        "element-plus/es/components/dropdown/style/css",
        "element-plus/es/components/icon/style/css",
        "element-plus/es/components/menu-item/style/css",
        "element-plus/es/components/menu/style/css",
        "element-plus/es/components/message/style/css",
        "element-plus/es/components/scrollbar/style/css",
        "element-plus/es/components/sub-menu/style/css",
        "element-plus/lib/locale/lang/en",
        "element-plus/lib/locale/lang/zh-cn",
        "js-cookie",
        "lodash-es",
        "path-to-regexp",
        "uc-lib",
        "vue",
        "vue-i18n",
        "vue-router",
        "vuex"
      ]
    }
  }
}
